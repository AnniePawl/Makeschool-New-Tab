{
  "name": "Makeschool Dashboard",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A New Tab dashboard for Makeschool students.",
  "homepage_url": "https://joerezendes.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/override.html"
  },
  "permissions": [ "https://source.unsplash.com/","http://quotes.rest/", "bookmarks"]
}
